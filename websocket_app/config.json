{
    "logging": {
        "level": "INFO",
        "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s",
        "file": "logs/app.log",
        "max_bytes": 10485760,
        "backup_count": 5,
        "console": true,
        "sound_debug": false
    },
    "auto_summary": {
        "sst_ws_url": "wss://rtsttws-demo.securityzone.vn",
        "n8n_webhook_url": "https://n8n.securityzone.vn/webhook/rt-auto-summary",
        "interval_seconds": 60,
        "min_summary_words": 30
    }
}